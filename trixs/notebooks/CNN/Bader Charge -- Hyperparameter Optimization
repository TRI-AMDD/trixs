    "      % ((x_test.shape), (y_test.shape)))\n",
    "\n",
    "# Want to normalize the targets first\n",
    "y_train = y_train/np.max(y_train, keepdims=True)\n",
    "y_valid = y_valid/np.max(y_valid, keepdims=True)\n",
    "y_test = y_test/np.max(y_test, keepdims=True)"
   ]
